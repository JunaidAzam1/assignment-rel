For(j=0;j<10;j++)

{

// Relation a to b , a R B iff a divides b

If(b[j]%a[i]==0)
printf(" %d %d ",a[i],b[j]);
}
}

// Relation a to b , a R B iff a equals to b

elseif(a[i]==b[j])
{

printf(" %d %d ",a[i],b[j]);
}

else
{
printf("invalid input ");
}
